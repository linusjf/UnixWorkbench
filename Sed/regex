#!/usr/bin/env bash
printf "grep -e '^  *.*' lines.txt\n"
grep -e '^  *.*' lines.txt
printf "grep -e 'A.E' lines.txt\n"
grep -e 'A.E' lines.txt
printf "grep -e 'A.*E' lines.txt\n"
grep -e 'A.*E' lines.txt
printf "grep -e '\.   ' lines.txt\n"
grep -e '\.   ' lines.txt
printf "grep -e '80.86' lines.txt\n"
grep -e '80.86' lines.txt
printf "grep -e 'Chapter.' lines.txt\n"
grep -e 'Chapter.' lines.txt
printf "grep -e '[Ww]hat' lines.txt\n"
grep -e '[Ww]hat' lines.txt
printf "grep -e '\.H[123]' lines.txt\n"
grep -e '\.H[123]' lines.txt
printf "grep -e '.[!?;:,\".] .' lines.txt\n"
grep -e '.[!?;:,".] .' lines.txt
printf "awk '/[a\]1]/ { print }' lines.txt\n"
awk '/[a\]1]/ { print }' lines.txt
printf "grep -e '[]a1]' lines.txt\n"
grep -e '[]a1]' lines.txt
printf "grep -e '[][ a-z]' lines.txt\n"
grep -e '[][ a-z]' lines.txt
printf "awk '/[[\] a-z]/ { print }' lines.txt\n"
awk '/[[\[ a-z]/ { print }' lines.txt
printf "grep -e '[A-Z]' lines.txt\n"
grep -e '[A-Z]' lines.txt
printf "grep -e '[0-9]' lines.txt\n"
grep -e '[0-9]' lines.txt
printf "grep -e '[cC]hapter [1-9]' lines.txt\n"
grep -e '[cC]hapter [1-9]' lines.txt
grep -e '[0-9a-z?,.;:’"]' lines.txt
printf "grep -e \"[0-9a-z?,.;:'\"]\" lines.txt\n"
grep -e "[0-9a-z?,.;:'\"]" lines.txt
printf "grep -e '[a-zA-Z][.?!]' lines.txt\n"
grep -e '[a-zA-Z][.?!]' lines.txt
printf "grep -e '[0-1][0-9][-/][0-3][0-9][-/][0-9][0-9]' lines.txt\n"
grep -e '[0-1][0-9][-/][0-3][0-9][-/][0-9][0-9]' lines.txt
printf "grep -e '[^0-9]' lines.txt\n"
grep -e '[^0-9]' lines.txt
printf "grep -e '[ˆaeiou]' lines.txt\n"
grep -e '[ˆaeiou]' lines.txt
printf "grep -e '\.DS \"[^1]\"' lines.txt\n"
grep -e '\.DS "[^1]"' lines.txt
printf "grep -e '[[:alnum:]]' lines.txt\n"
grep -e '[[:alnum:]]' lines.txt
printf "grep -e '[[:alpha:]]' lines.txt\n"
grep -e '[[:alpha:]]' lines.txt
printf "grep -e '[[:blank:]]' lines.txt\n"
grep -e '[[:blank:]]' lines.txt
printf "grep -e '[[:cntrl:]]' lines.txt | cat -A\n"
grep -e '[[:cntrl:]]' lines.txt | cat -A
printf "grep -e '[[:digit:]]' lines.txt\n"
grep -e '[[:digit:]]' lines.txt
printf "grep -e '[[:graph:]]' lines.txt\n"
grep -e '[[:graph:]]' lines.txt
printf "grep -e '[[:lower:]]' lines.txt\n"
grep -e '[[:lower:]]' lines.txt
printf "grep -e '[[:print:]]' lines.txt\n"
grep -e '[[:print:]]' lines.txt
printf "grep -e '[[:punct:]]' lines.txt\n"
grep -e '[[:punct:]]' lines.txt
printf "grep -e '[[:space:]]' lines.txt\n"
grep -e '[[:space:]]' lines.txt
printf "grep -e '[[:upper:]]' lines.txt\n"
grep -e '[[:upper:]]' lines.txt
printf "grep -e '[[:xdigit:]]' lines.txt\n"
grep -e '[[:xdigit:]]' lines.txt
printf "grep -e '<.*>' lines.txt\n"
grep -e '<.*>' lines.txt
printf "grep -e \"can[ no']*t\" lines.txt\n"
grep -e "can[ no']*t" lines.txt
printf "grep -E '80[234]?86' lines.txt\n"
grep -E '80[2-4]?86' lines.txt
